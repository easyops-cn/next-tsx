{
  "name": "@next-tsx/converter",
  "version": "0.7.7",
  "homepage": "https://github.com/easyops-cn/next-tsx/tree/master/packages/converter",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-tsx.git",
    "directory": "packages/converter"
  },
  "license": "GPL-3.0",
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "lib",
    "src/helpers"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prestart": "npm run clean",
    "start": "concurrently -k -n types,main \"npm run start:types\" \"npm run start:main\"",
    "start:main": "cross-env NODE_ENV=development build-next-libs --watch",
    "start:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types --project tsconfig.build.json --watch",
    "prebuild": "npm run clean",
    "build": "npm run build:types && npm run build:main",
    "build:main": "cross-env NODE_ENV=production build-next-libs",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types --project tsconfig.build.json",
    "test": "cross-env NODE_ENV='test' test-next",
    "test:ci": "cross-env NODE_ENV='test' CI=true test-next"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@babel/types": "^7.27.7",
    "@easyops-cn/brick-next-pipes": "^0.7.3",
    "@next-api-sdk/cmdb-sdk": "^1.1.1",
    "@next-core/cook": "^2.5.12",
    "@next-core/types": "^1.19.0",
    "@next-core/utils": "^1.8.9",
    "@next-tsx/parser": "^0.10.0"
  },
  "devDependencies": {
    "@next-core/build-next-libs": "^1.0.25",
    "@next-core/test-next": "^2.0.1"
  }
}
